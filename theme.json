				{
					"color": "#FFFFFF",
					"name": "White",
					"slug": "white"
				}
			]
		},
		"custom": {
			"color": {
				"body": {
					"text": "var(--wp--preset--color--typography)",
					"background": "var(--wp--preset--color--white)",
					"backgroundMain": "var(--wp--preset--color--white)",
					"backgroundKnockout": "var(--wp--preset--color--knockout)",
					"accent": "var(--wp--preset--color--accent)",
					"accentContrast": "var(--wp--preset--color--negative)",
					"divider": "var(--wp--preset--color--accent)"
				},
				"button": {
					"text": "var(--wp--preset--color--negative)",
					"background": "var(--wp--preset--color--accent)"
				},
				"header": {
					"text": "var(--wp--preset--color--typography)",
					"background": "var(--wp--preset--color--primary)",
					"backgroundSecondary": "var(--wp--preset--color--white)",
					"border": "var(--wp--preset--color--secondary)",
					"searchIconColor": "var(--wp--preset--color--typography)",
					"searchIconBackground": "transparent",
					"searchBoxText": "var(--wp--preset--color--typography)",
					"searchBoxBackground": "#fff"
				},
				"footer": {
					"text": "var(--wp--preset--color--typography)",
					"background": "var(--wp--preset--color--white)",
					"border": "var(--wp--preset--color--secondary)",
					"divider": "var(--wp--preset--color--accent)"
				}
			},
			"text": {
				"base": {
					"family": "Raleway, sans-serif",
					"weight": "400"
				},
				"heading": {
					"family": "Raleway, sans-serif",
					"weight": "800"
				}
			}
		},
		"dimensions": {
			"aspectRatios": [
				{
					"name": "Post",
					"ratio": "5/3",
					"slug": "post"
				},
				{
					"name": "Cover",
					"ratio": "6/2",
					"slug": "cover"
				}
			]
		},
		"layout": {
			"contentSize": "1000px",
			"wideSize": "1000px"
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "X-small",
					"size": ".75rem",
					"slug": "xs"
				},
				{
					"name": "Small",
					"size": "1rem",
					"slug": "sm"
				},
				{
					"name": "Base",
					"size": "1.5rem",
					"slug": "base"
				},
				{
					"name": "Medium",
					"size": "clamp(1.75rem, calc(1.75rem + ((1vw - 0.48rem) * 0.7813)), 2rem);",
					"slug": "md"
				},
				{
					"name": "Large",
					"size": "clamp(2.25rem, calc(2.25rem + ((1vw - 0.48rem) * 1.9531)), 2.875rem);",
					"slug": "lg"
				},
				{
					"name": "X-Large",
					"size": "clamp(3.125rem, calc(3.125rem + ((1vw - 0.48rem) * 9.7656)), 6.25rem);",
					"slug": "xl"
				},
				{
					"name": "2x-Large",
					"size": "clamp(6.25rem, calc(6.25rem + ((1vw - 0.48rem) * 19.5313)), 12.5rem);",
					"slug": "2xl"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Raleway",
							"fontStyle": "normal",
							"fontWeight": "200",
