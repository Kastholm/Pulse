				},
				{
					"color": "#E9EFF2",
					"name": "secondary",
					"slug": "Secondary"
				},
				{
					"color": "#3D66AF",
					"name": "accent",
					"slug": "accent"
				},
				{
					"color": "#f9fafb",
					"name": "Negative",
					"slug": "negative"
				},
				{
					"color": "#f5f5f5",
					"name": "Knockout",
					"slug": "knockout"
