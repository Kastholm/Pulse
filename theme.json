						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": "file:./src/fonts/raleway-v34-latin-italic.woff2"
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": "file:./src/fonts/raleway-v34-latin-600.woff2"
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": "file:./src/fonts/raleway-v34-latin-600italic.woff2"
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": "file:./src/fonts/raleway-v34-latin-800.woff2"
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "italic",
							"fontWeight": "800",
							"src": "file:./src/fonts/raleway-v34-latin-800italic.woff2"
						}
					],
					"fontFamily": "'Raleway', sans-serif",
					"name": "Raleway",
					"slug": "nunito-sans"
				}
			],
			"fluid": true,
			"fontSizes": [
				{
					"fluid": {
						"max": "14px",
						"min": "12px"
					},
					"name": "X-small",
					"size": "12",
					"slug": "xs"
				},
				{
					"fluid": {
						"max": "16px",
						"min": "14px"
					},
					"name": "Small",
					"size": "14",
					"slug": "sm"
				},
				{
					"fluid": {
						"max": "16px",
						"min": "16px"
					},
					"name": "Base",
					"size": "16",
					"slug": "base"
				},
				{
					"fluid": {
						"max": "20px",
